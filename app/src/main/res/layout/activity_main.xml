<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <ImageView
            android:layout_width="48dp"
            android:layout_height="48dp" app:srcCompat="@drawable/rocket" tools:layout_editor_absoluteY="47dp"
            tools:layout_editor_absoluteX="49dp" android:id="@+id/imageViewEnemy"/>
    <ImageView
            android:layout_width="48dp"
            android:layout_height="48dp" app:srcCompat="@drawable/arw03up" tools:layout_editor_absoluteY="356dp"
            tools:layout_editor_absoluteX="40dp" android:id="@+id/imageViewPlayer"/>
    <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp" app:srcCompat="@drawable/arw02up" tools:layout_editor_absoluteY="213dp"
            tools:layout_editor_absoluteX="64dp" android:id="@+id/imageViewBullet"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="443dp"
            tools:layout_editor_absoluteX="88dp" android:id="@+id/textView"/>
</android.support.constraint.ConstraintLayout>